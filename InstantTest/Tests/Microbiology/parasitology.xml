        <options>
          <option type="text" answer="false" weight=".00%">ascariasis</option>
          <option type="text" answer="false" weight=".00%">malaria</option>
          <option type="text" answer="false" weight=".00%">dracunculiasis</option>
          <option type="text" answer="false" weight=".00%">hymenolepiasis.</option>
