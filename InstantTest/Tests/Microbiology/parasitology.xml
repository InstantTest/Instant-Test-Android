  <sections>
    <section format="multiple choice" instruction="">
      <question number="1">
        <text type="text">Diphyllobothrium latum is:</text>
        <options>
          <option type="text" answer="false" weight=".00%">a pseudophyllidian tapeworm</option>
          <option type="text" answer="false" weight=".00%">a true nematode</option>
          <option type="text" answer="false" weight=".00%">related to the Ctenocephalides species</option>
          <option type="text" answer="false" weight=".00%">not a worm that has a definite taxonomic status.</option>
        </options>
        <reason type="text" />
      </question>
      <question number="2">
        <text type="text">Helminthic infections are generally characterized by:</text>
        <options>
          <option type="text" answer="false" weight=".00%">insomnia, dermatitis and loss of hair</option>
          <option type="text" answer="false" weight=".00%">increase in eosinophil numbers in the blood</option>
          <option type="text" answer="false" weight=".00%">increase in the number of monocytes in the blood</option>
          <option type="text" answer="false" weight=".00%">elephantiasis.</option>
        </options>
        <reason type="text" />
      </question>
      <question number="3">
